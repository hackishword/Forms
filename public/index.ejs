<!-- # Full Width -->
<!-- & <p>If you  don't add a form-panel-SIZE class, it'll take up 100% width.</p> -->
<!-- : ; 100% -->
<form class="form form-panel">
  <div class="form-panel-header">
    <h3>Sign In</h3>
  </div>
  <div class="form-panel-body">
    <label for="login4">Email</label>
    <input class="form-input-block" id="login4" name="login" type="text">
    <label for="password4">Password</label>
    <input class="form-input-block" id="password4" name="password" type="password">
    <button class="button button-block button-rounded button-primary">Sign In</button>
  </div>
</form>

<!-- @ Grouped -->
<!-- # Rectangular -->
<!-- : ; 100% -->
<form class="form">
  <fieldset class="form-grouped">
    <input name="username" type="text" placeholder="Username">
    <input name="login" type="text" placeholder="Email">
    <input name="password" type="password" placeholder="Password">
  </fieldset>
  <button class="button button-block button-primary button-full">Sign In</button>
</form>

<!-- # Rounded -->
<!-- : ; 100% -->
<form class="form">
  <fieldset class="form-grouped form-grouped-rounded">
    <div class="form-field-group icon-right">
      <input name="username" type="text" placeholder="Username">
      <i class="fa fa-user"></i>
    </div>
    <div class="form-field-group icon-right">
      <input name="login" type="text" placeholder="Email">
      <i class="fa fa-envelope"></i>
    </div>
    <div class="form-field-group icon-right">
      <input name="password" type="password" placeholder="Password">
      <i class="fa fa-lock"></i>
    </div>
  </fieldset>
  <button class="button button-block button-rounded button-primary button-full">Sign In</button>
</form>

<!-- # Panel -->
<!-- : ; 100% -->
<form class="form form-panel panel-demo-login">
  <div class="form-panel-header">
    <h3>Sign In</h3>
  </div>
  <div class="form-panel-body">
    <fieldset class="form-grouped form-grouped-block form-grouped-rounded">
      <input name="username" type="text" placeholder="Username">
      <input name="login" type="text" placeholder="Email">
      <input name="password" type="password" placeholder="Password">
    </fieldset>
    <a href="#" class="button button-block button-primary button-rounded">Sign In</a>
  </div>
</form>

<!-- @ Inline -->
<!-- # Inline Form -->
<!-- : ; 100% -->
<form class="form form-panel">
  <div class="form-panel-header">
    <h2>Inline Form</h2>
  </div>
  <div class="form-panel-body">
    <label>Email</label>
    <input type="text">
    <label>Password</label>
    <input type="password">
    <input id="remember" type="checkbox">
    <label for="remember">Remember me</label>
    <button class="button button-rounded button-primary">Sign In</button>
  </div>
</form>

<!-- @ Inputs -->
<!-- # Core -->
<!-- : ; 100% -->
<ul>
  <li class="form">
    <div class="form-field-group icon-left">
      <input class="form-field-box clearfix" type="text" name="text" placeholder="Icon Left">
      <i class="fa fa-user"></i>
    </div>
  </li>
  <li class="form">
    <div class="form-field-group icon-right">
      <input class="form-field-box clearfix" type="password" name="text" placeholder="Icon Right">
      <i class="fa fa-lock"></i>
    </div>
  </li>
  <li class="form">
    <div class="form-field-group">
      <input class="form-field-box clearfix" type="text" name="text" placeholder="Box Input">
    </div>
  </li>
  <li class="form">
    <div class="form-field-group">
      <input class="form-field clearfix" type="text" name="text" placeholder="Vanilla Input">
    </div>
  </li>
  <li class="form">
    <div class="form-field-group">
      <input class="form-field-pill clearfix" type="text" name="text" placeholder="Pill Input">
    </div>
  </li>
  <li class="form">
    <div class="form-field-group">
      <input class="form-field-line clearfix" type="text" name="text" placeholder="Line Input">
    </div>
  </li>
</ul>

<!-- # HTML5 -->
<!-- : ; 100% -->
<ul>
  <li class="form">
    <div class="form-field-group">
      <input class="email form-field clearfix" type="email" name="email" placeholder="Email">
    </div>
  </li>
  <li class="form">
    <div class="form-field-group">
      <input class="password form-field clearfix" type="password" name="password" placeholder="Password">
